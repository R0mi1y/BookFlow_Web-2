{
  "expo": {
    "name": "book flow",
    "slug": "book_flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c161e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bookflow",
      "config": {}
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "bookflow",
    "extra": {
      "apiUrl": "http://89.117.75.69:8881",
      "eas": {
        "projectId": "1b18319f-92e7-4d30-927d-eb62eae6fee3"
      }
    },
    "owner": "romily",
    "plugins": [
      "expo-secure-store"
    ]
  },
  "web": "expo start --web"
}
