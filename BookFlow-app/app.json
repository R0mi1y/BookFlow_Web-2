{
  "expo": {
    "name": "book flow",
    "slug": "book-flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c161e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookflow"
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bookflow",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB2YzngwGwvTwI5DRVySMBfJ-6TzLPjl1Q"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "bookflow",
    "extra": {
      "apiUrl": "http://89.117.75.69:8881",
      "eas": {
        "projectId": "54606835-cb6d-4e34-a7af-34b5127b0813"
      }
    },
    "owner": "ediel",
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  },
  "web": "expo start --web"
}
